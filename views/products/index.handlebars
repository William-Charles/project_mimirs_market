<header class="page-header">
  <h1>Products</h1>
</header>


{{#if products.length }}
  <div class="row">
    {{#each products as |product| }}
      <div class="col-sm-4">
        <a href="/products/{{ product.id }}">
          <div class="thumbnail">
  					<img src="http://i.imgur.com/qK42fUu.jpg">
            <h6>{{product.name}}</h6>
  				</div>
        </a>
      </div>
    {{/each }}
  </div>


{{else }}
  <p class="text-danger">No products</p>
{{/if }}
